{
  "name": "adonis-on-fire",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "cd src && node ace build && cp .env.example ../build/.env",
    "deploy": "firebase deploy --only functions",
    "dev": "node ace serve --watch",
    "format": "prettier --write .",
    "lint": "eslint --ext .js,.ts --config src/.eslintrc.json",
    "logs": "firebase functions:log",
    "run": "cd build && node index.js",
    "serve": "npm run build && firebase emulators:start --only functions",
    "shell": "npm run build && firebase functions:shell",
    "start": "npm run shell"
  },
  "engines": {
    "node": "16"
  },
  "main": "build/index.js",
  "dependencies": {
    "@google-cloud/error-reporting": "^2.0.4",
    "@google-cloud/storage": "^5.7.0",
    "@google-cloud/tasks": "^2.3.5",
    "base32": "0.0.6",
    "base64url": "^3.0.1",
    "bluebird": "^3.7.2",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chai-subset": "^1.6.0",
    "csv-stringify": "^5.6.0",
    "error-control": "^0.2.5",
    "express": "5.0.0-alpha.8",
    "firebase": "^8.6.8",
    "firebase-admin": "^9.10.0",
    "firebase-extra": "^0.4.2",
    "firebase-functions": "^3.15.4",
    "flat": "^5.0.2",
    "immer": "^9.0.3",
    "ix": "^4.3.1",
    "lodash": "^4.17.20",
    "luxon": "^1.25.0",
    "ngeohash": "^0.6.3",
    "pretty-ms": "^7.0.1",
    "sqlite3": "^5.0.0",
    "supertest": "^6.0.1",
    "tmp": "^0.2.1",
    "winston": "^3.2.1",
    "yargs": "^12.0.1",
    "@google-cloud/logging-winston": "^3.0.0"
  },
  "devDependencies": {
    "@firebase/rules-unit-testing": "^1.3.8",
    "eslint": "^5.12.0",
    "eslint-plugin-promise": "^4.0.1",
    "firebase-functions-test": "^0.3.2",
    "mocha": "^8.2.0"
  }
}
